<?xml version="1.0" encoding="utf-8"?>
<com.onlylady.beautyapp.views.RippleView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:background="@color/white"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/layout_title"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:background="@color/dedede">

        <ImageView

            android:id="@+id/goback"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_centerVertical="true"
            android:padding="12dp"
            android:src="@mipmap/loginbackicon" />

        <TextView
            android:id="@+id/title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="123"
            android:textColor="@color/colorff3064"
            android:textSize="17sp" />

    </RelativeLayout>

    <com.handmark.pulltorefresh.library.PullToRefreshRecycleView


        android:id="@+id/pulltorefrshlistview_live"

        style="@style/listview"
        android:layout_below="@+id/layout_title">

    </com.handmark.pulltorefresh.library.PullToRefreshRecycleView>

    <com.onlylady.beautyapp.exlib.fab.FloatingActionButton
        android:id="@+id/fab"
        android:layout_width="@dimen/fabbarheight"
        android:layout_height="@dimen/fabbarheight"
        android:scaleType="fitXY"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        app:floatingActionButtonColor="@color/transparent"
        android:layout_marginBottom="20dp"
        app:floatingActionButtonShadow="false"
        android:layout_marginRight="10dp"
        app:floatingActionButtonSize="mini"
        android:src="@mipmap/gototop">

    </com.onlylady.beautyapp.exlib.fab.FloatingActionButton>



</com.onlylady.beautyapp.views.RippleView>
